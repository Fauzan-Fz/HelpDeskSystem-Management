@model HelpDeskSystem.Models.UserRoleProfile

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>UserRoleProfile</h4>
<hr />
<div class="card rounded-4 mt-4">
    <div class="card-body">
        <div class="d-flex align-items-center">
            <h4 class="mb-3c">Add New SystemSetting</h4>
        </div>
        <hr />
        <div class="row">
            <div class="col-md-12">
                <form asp-action="Create">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="TaskId" class="control-label"></label>
                                <select asp-for="TaskId" class="form-control" asp-items="ViewBag.TaskId"></select>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label asp-for="RoleId" class="control-label"></label>
                                <select asp-for="RoleId" class="form-control" asp-items="ViewBag.RoleId"></select>
                            </div>
                        </div>
                    </div>
                    <div class="form-group mt-3">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
                    <div class="mt-3">
                        <a class="btn btn-warning" asp-action="Index">Back to List</a>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}